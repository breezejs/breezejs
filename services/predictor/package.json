{
  "name": "@breeze/predictor",
  "version": "0.0.0",
  "description": "Breeze football predictor API",
  "author": "Mo Gusbi <me@mogusbi.co.uk>",
  "homepage": "https://github.com/mogusbi/breeze",
  "license": "MIT",
  "main": "lib/main.js",
  "typings": "lib/main.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mogusbi/breeze.git"
  },
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --options ../../typedoc.js --out ./docs ./src",
    "e2e": "jest --config=e2e/jest.config.js --runInBand --forceExit",
    "lint": "tslint -c ../../tslint.json -p tsconfig.json",
    "start": "nodemon --exec ts-node src/main.ts",
    "test": "jest --coverage",
    "posttest": "codecov"
  },
  "dependencies": {
    "@breeze/bootstrap": "^0.0.0",
    "@breeze/competition": "^0.0.0",
    "@breeze/fixture": "^0.0.0",
    "@breeze/request": "^0.0.0",
    "@breeze/season": "^0.0.0",
    "@breeze/sql": "^0.0.0",
    "@breeze/team": "^0.0.0",
    "mysql": "^2.16.0",
    "typeorm": "^0.2.9"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.ts",
      "!**/e2e/**/*.ts",
      "!**/src/**/*.dto.ts",
      "!**/src/**/*.entity.ts",
      "!**/src/**/*.model.ts",
      "!**/src/**/*.module.ts",
      "!**/src/**/*.providers.ts"
    ],
    "coveragePathIgnorePatterns": [
      "index.ts",
      "main.ts",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "@breeze/competition": "<rootDir>/node_modules/@breeze/competition/lib/__mocks__",
      "@breeze/fixture": "<rootDir>/node_modules/@breeze/fixture/lib/__mocks__",
      "@breeze/season": "<rootDir>/node_modules/@breeze/season/lib/__mocks__",
      "@breeze/team": "<rootDir>/node_modules/@breeze/team/lib/__mocks__"
    },
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts|js)?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
